<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.choidaye.readyforbox">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>

    <application
            android:name=".Network.ApplicationController"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme"
            android:usesCleartextTraffic="true">
        <activity android:name=".UI.Activity.Order.OrderActivity">
        </activity>
        <activity android:name=".UI.Activity.Mypage.MyPageDeliveryManangerActivity">
        </activity>
        <activity android:name=".UI.Activity.MainActivity">
        </activity>
        <activity android:name=".UI.Activity.Cart.CartActivity">
        </activity>
        <activity android:name=".UI.Activity.Sign.SignupActivity">
        </activity>
        <activity android:name=".UI.Activity.Order.OrdercompletedActivity">
        </activity>
        <activity android:name=".UI.Activity.TutorialActivity">
        </activity>
        <activity android:name=".UI.Activity.Sign.LoginActivity">
        </activity>
        <activity android:name=".UI.Activity.Sign.SignupCartActivity">
        </activity>
        <activity android:name=".UI.Activity.ForUSettingActivity">
        </activity>
        <activity
                android:name=".UI.Activity.SplashActivity"
                android:screenOrientation="portrait"
                android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>



    </application>

</manifest>